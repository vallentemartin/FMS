                                    <div class="info-box mb-3">
                                        <span class="info-box-icon boxiconInactive"><i class="far fa-file-alt"></i></span>
                                        <div class="info-box-content">
                                            <span class="info-box-text" id="terminatedContracts"></span>
                                            <span class="info-box-number terminatedContracts"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix hidden-md-up"></div>
                            </div>
                        </div>
                    </div>
