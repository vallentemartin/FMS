<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12">
                <div class="card card-lightblue">
                    <div class="card-header">
                        <h3 class="card-title">List Of Packinghouse</h3>
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                            @*<button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>*@
                        </div>
                    </div>
                    <div class="card-body">
                        <table id="tbl_Packinghouse" class="table table-bordered table-striped table-sm" style="width:100%" data-editpage="pages/masterdata/modalpages/Packinghouse_update" data-addpage="pages/masterdata/modalpages/Packinghouse_add">
                            <thead>
                                <tr style="text-align:center">
                                    <th class="id">Actions</th>
                                    <th class="PackinghouseName">Packinghouse Name</th>
                                    <th class="AreaCode">Area</th>
                                    <th class="PackinghouseAddress">Address</th>
                                    <th class="ContactPerson">Contact Person</th>
                                    <th class="ContactPersonNo">Contact #</th>
                                    <th class="isactive">Status</th>
                                    <th class="modifiedby">Last Modified By</th>
                                    <th class="modifiedon">Last Modified</th>
                                </tr>
                            </thead>
                            <tbody id="tbl_Users-data">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/MasterData/Packinghouse.js"></script>